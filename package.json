{
  "name": "@sroussey/parse-full-name",
  "version": "1.0.12",
  "description": "A function that accepts the full name of a person in any format, parses it, and returns its parts { title, first name, middle name, last name, nickname, suffix, [and any parsing errors] }.",
    "scripts": {
    "test": "bun test",
    "build": "bun run build-clean && bun run build-js && bun run build-types",
    "build-clean": "rm -rf dist tsconfig.tsbuildinfo",
    "build-js": "bun build src/index.ts --outdir dist",
    "build-types": "tsc --project tsconfig.json --emitDeclarationOnly"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sroussey/parse-full-name"
  },
  "keywords": [
    "parse",
    "parser",
    "name",
    "full name",
    "first name",
    "middle name",
    "last name",
    "nickname"
  ],
  "author": "Own Up <dev@ownup.com> (https://www,ownup.com)",
  "contributors": [
    "David Schnell-Davis <dschnelldavis@gmail.com> (http://github.com/dschnelldavis)",
    "Steve Roussey <sroussey@gmail.com> (http://github.com/sroussey)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sroussey/parse-full-name/issues"
  },
  "homepage": "https://github.com/sroussey/parse-full-name",
  "dependencies": {},
  "devDependencies": {
    "typescript": "^5.8.3",
    "@types/bun": "^1.2.15"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "type": "module",
  "engines": {
    "bun": ">=1.2.15"
  },
  "packageManager": "bun@1.2.15"
}
